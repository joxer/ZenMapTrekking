    <groundspeak:cache id="438366" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
      <groundspeak:name>Komm in' Gang!</groundspeak:name>
      <groundspeak:placed_by>ksmichel</groundspeak:placed_by>
      <groundspeak:owner id="119919">ksmichel</groundspeak:owner>
