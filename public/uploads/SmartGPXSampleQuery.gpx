      <groundspeak:name>Alster</groundspeak:name>
      <groundspeak:placed_by>burkie</groundspeak:placed_by>
      <groundspeak:owner id="137475">burkie</groundspeak:owner>
      <groundspeak:type>Multi-cache</groundspeak:type>
      <groundspeak:container>Micro</groundspeak:container>
